CA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"evaluate.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div class=\"evaluate\">\n    <div class=\"satisfaction\">\n      <h1>{{ext1}}</h1>\n      <article>\n        <div v-for=\"item in detailsOutputList\" class=\"noBox\" @click=\"activeUnLike\" v-if=\"item.evaluateType === 1 && item.sortOrder === 1\">\n          <img v-if=\"item.number === 0\" src=\"../assets/images/icon_dissatisfied_n.png\" alt=\"\">\n          <img v-else src=\"../assets/images/icon_dissatisfied_h.png\" alt=\"\">\n          <span :class=\"item.number ? 'unLikeActive' : ''\">{{item.items}}</span>\n        </div>\n        <div v-for=\"item in detailsOutputList\" class=\"yesBox\" @click=\"activeLike\" v-if=\"item.evaluateType === 1 && item.sortOrder === 2\">\n          <img v-if=\"item.number === 0\" src=\"../assets/images/Fill.png\" alt=\"\">\n          <img v-else src=\"../assets/images/icon_satisfied_h.png\" alt=\"\">\n          <span :class=\"item.number ? 'likeActive' : ''\">{{item.items}}</span>\n        </div>\n      </article>\n    </div>\n    <div class=\"fiveStar\">\n      <h2>{{ext2}}</h2>\n      <article v-for=\"item in detailsOutputList\" v-if=\"item.evaluateType === 2\">\n        <span>{{item.items}}</span>\n        <img v-for=\"i in item.number\" src=\"../assets/images/icon_star_h.png\" alt=\"\" @click=\"changeStar(item.id,i)\">\n        <img v-for=\"i in 5 - item.number\" src=\"../assets/images/icon_star_n.png\" alt=\"\" @click=\"changeStar(item.id,i,item.number)\">\n        <i>{{item.number | starText}}</i>\n      </article>\n    </div>\n    <div class=\"labelBox\">\n      <h2>{{ext3}}</h2>\n      <article>\n        <span :class=\"item.number ? 'active' : ''\" @click=\"changeLabel(item.id)\" v-for=\"item in detailsOutputList\" v-if=\"item.evaluateType === 3\">{{item.items}}</span>\n      </article>\n    </div>\n    <div class=\"textareaBox\">\n      <h2>请分享你的感受</h2>\n      <textarea v-if=\"openComment\" name=\"name\" placeholder=\"请输入您对本次服务的评价\" v-model=\"comment\"></textarea>\n    </div>\n    <div class=\"cantEva\" @click=\"submit\">提交评价</div>\n  </div>\n</template>\n\n<script>\n  import {\n    getEvaluateByEvaluatedId,\n    getEvaluateList,\n    saveEvaluate\n  } from \"../service/getData\";\n\n  export default {\n    name: 'evaluate',\n    data() {\n      return {\n        id: '',\n        cityId: '',\n        isEvaluate: '', //0未评价1已评价\n        marketId: '',\n        openComment: '', //1需要评语0不需要\n        detailsOutputList: '',\n        radioStatus: 1,\n        comment: '',\n        ext1: '',\n        ext2: '',\n        ext3: ''\n      }\n    },\n    created() {\n      this.getEvaluateByEvaluatedId()\n    },\n    methods: {\n      getEvaluateByEvaluatedId: function() {\n        getEvaluateByEvaluatedId(this.$route.params.id, this.$route.params.type).then(res => {\n          if (res.res.code === 10000) {\n            this.cityId = res.body.cityId\n            this.isEvaluate = res.body.isEvaluate\n            this.marketId = res.body.marketId\n            if (res.body.isEvaluate == 1) {\n              this.$router.push({\n                name: 'evaluateSuccess/0'\n              })\n            } else {\n              this.getEvaluateList(res.body.marketId, res.body.cityId, this.$route.params.type)\n            }\n          }\n        })\n      },\n      getEvaluateList: function(marketId, cityId, type) {\n        getEvaluateList(marketId, cityId, type).then(res => {\n          this.id = res.body.template.id\n          this.openComment = res.body.template.openComment\n          this.detailsOutputList = res.body.template.detailsOutputList\n          this.ext1 = res.body.template.ext1\n          this.ext2 = res.body.template.ext2\n          this.ext3 = res.body.template.ext3\n        })\n      },\n      activeUnLike: function() {\n        let detailsOutputList = this.detailsOutputList\n        for (var i = detailsOutputList.length; i--;) {\n          if (detailsOutputList[i].evaluateType === 1 && detailsOutputList[i].sortOrder === 1) {\n            detailsOutputList[i].number = 1\n          }\n          if (detailsOutputList[i].evaluateType === 1 && detailsOutputList[i].sortOrder === 2) {\n            detailsOutputList[i].number = 0\n          }\n        }\n        this.detailsOutputList = detailsOutputList\n      },\n      activeLike: function() {\n        let detailsOutputList = this.detailsOutputList\n        for (var i = detailsOutputList.length; i--;) {\n          if (detailsOutputList[i].evaluateType === 1 && detailsOutputList[i].sortOrder === 1) {\n            detailsOutputList[i].number = 0\n          }\n          if (detailsOutputList[i].evaluateType === 1 && detailsOutputList[i].sortOrder === 2) {\n            detailsOutputList[i].number = 1\n          }\n        }\n        this.detailsOutputList = detailsOutputList\n      },\n      changeLabel: function(id) {\n        let detailsOutputList = this.detailsOutputList\n        for (var i = detailsOutputList.length; i--;) {\n          if (detailsOutputList[i].id === id) {\n            if (detailsOutputList[i].number) {\n              detailsOutputList[i].number = 0\n            } else {\n              detailsOutputList[i].number = 1\n            }\n          }\n        }\n        this.detailsOutputList = detailsOutputList\n      },\n      changeStar: function(id, index, activeLength) {\n        let detailsOutputList = this.detailsOutputList\n\n        for (var i = detailsOutputList.length; i--;) {\n          if (detailsOutputList[i].id === id) {\n            if (activeLength) {\n              detailsOutputList[i].number = index + activeLength\n            } else {\n              detailsOutputList[i].number = index\n            }\n          }\n        }\n        this.detailsOutputList = detailsOutputList\n      },\n      submit: function() {\n        saveEvaluate(this.id, this.$route.params.id, JSON.stringify(this.detailsOutputList), this.comment).then(res => {\n          this.$router.push({\n            name: 'evaluateSuccess/1'\n          })\n        })\n      }\n    },\n    filters: {\n      starText: function(value) {\n        switch (value) {\n          case 1:\n            return '非常不满意'\n            break;\n          case 2:\n            return '不满意'\n            break;\n          case 3:\n            return '一般'\n            break;\n          case 4:\n            return '满意'\n            break;\n          case 5:\n            return '非常满意'\n            break;\n          default:\n\n        }\n      }\n    }\n  }\n</script>\n\n<style scoped lang=\"scss\">\n  .evaluate {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    min-height: 100vh;\n\n    .satisfaction {\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n\n      h1 {\n        font-size: .43rem;\n        font-family: PingFangSC-Medium;\n        color: rgba(51, 51, 51, 1);\n        line-height: .59rem;\n        padding-top: .53rem;\n      }\n\n      article {\n        display: flex;\n        margin-top: .53rem;\n        div {\n          display: flex;\n          flex-direction: column;\n          align-items: center;\n          margin: 0 1.15rem;\n          img {\n            width: .91rem;\n            height: .91rem;\n          }\n          span {\n            padding-top: .19rem;\n            font-size: .32rem;\n            font-family: PingFangSC-Regular;\n            color: rgba(51, 51, 51, 1);\n          }\n          .unLikeActive {\n            color: #6B88B2;\n          }\n          .likeActive {\n            color: #FF7800;\n          }\n        }\n      }\n    }\n\n    .fiveStar {\n      display: flex;\n      flex-direction: column;\n      padding: 0 .53rem;\n      width: 8.93rem;\n\n      h2 {\n        font-size: .43rem;\n        font-family: PingFangSC-Medium;\n        color: rgba(51, 51, 51, 1);\n        padding-top: 1.07rem;\n        margin-bottom: .16rem;\n      }\n\n      article {\n        display: flex;\n        align-items: center;\n        height: .53rem;\n        margin-top: .37rem;\n\n        span {\n          font-size: .37rem;\n          font-family: PingFangSC-Regular;\n          color: rgba(51, 51, 51, 1);\n        }\n\n        img {\n          width: .53rem;\n          height: .53rem;\n          margin-left: .27rem;\n        }\n\n        i {\n          font-size: .32rem;\n          font-family: PingFangSC-Regular;\n          color: rgba(255, 120, 0, 1);\n          padding-left: .19rem;\n        }\n      }\n    }\n\n    .labelBox {\n      display: flex;\n      flex-direction: column;\n      padding: 0 .53rem;\n      width: 8.93rem;\n\n      h2 {\n        font-size: .43rem;\n        font-family: PingFangSC-Medium;\n        color: rgba(51, 51, 51, 1);\n        padding-top: 1.07rem;\n        margin-bottom: .53rem;\n      }\n\n      article {\n        display: flex;\n        flex-wrap: wrap;\n        span {\n          padding: 0 .27rem;\n          height: .64rem;\n          border-radius: .37rem;\n          border: 1px solid rgba(218, 218, 218, 1);\n          line-height: .64rem;\n          text-align: center;\n          margin-bottom: .19rem;\n          margin-right: .19rem;\n          font-size: .32rem;\n          font-family: PingFangSC-Regular;\n          color: rgba(153, 153, 153, 1);\n        }\n\n        .active {\n          background: rgba(0, 189, 255, 0.1);\n          border: 1px solid rgba(0, 189, 255, 0.3);\n          font-family: PingFangSC-Regular;\n          color: rgba(0, 189, 255, 1);\n        }\n      }\n    }\n\n    .textareaBox {\n      display: flex;\n      flex-direction: column;\n      padding: 0 .37rem;\n      width: 9.25rem;\n\n      h2 {\n        padding-left: 0.53rem - 0.37rem;\n        font-size: .43rem;\n        font-family: PingFangSC-Medium;\n        color: rgba(51, 51, 51, 1);\n        padding-top: 1.07rem;\n        margin-bottom: .53rem;\n      }\n\n      textarea {\n        width: 8.88rem;\n        height: 2.83rem;\n        background: rgba(252, 252, 252, 1);\n        border-radius: .11rem;\n        border: 1px solid rgba(234, 234, 234, 1);\n        padding: .19rem;\n      }\n    }\n\n    .cantEva {\n      width: 8.4rem;\n      height: 1.01rem;\n      background:linear-gradient(270deg,rgba(0,154,255,1) 0%,rgba(0,189,255,1) 100%);\n      border-radius: .51rem;\n      margin-top: .75rem;\n      font-size: .45rem;\n      font-family: PingFangSC-Medium;\n      color: rgba(255, 255, 255, 1);\n      line-height: 1.01rem;\n      text-align: center;\n      margin-bottom: 0.5rem;\n    }\n  }\n</style>\n"]}]}